{
    "name": "ansible",
    "displayName": "language-Ansible",
    "description": "Ansible syntax highlighting support",
    "version": "0.2.0",
    "publisher": "haaaad",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Languages"
    ],
    "license": "MIT",
    "icon": "images/ansible.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/haad/vs-language-ansible.git"
    },
    "bugs": {
        "url": "https://github.com/haad/vs-language-ansible/issues"
    },
    "contributes": {
        "languages": [
            {
                "id": "ansible",
                "aliases": [
                    "Ansible",
                    "ansible"
                ],
                "extensions": [
                    ""
                ],
                "configuration": "./ansible.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "ansible",
                "scopeName": "source.ansible",
                "path": "./syntaxes/ansible.tmLanguage"
            }
        ]
    }
}