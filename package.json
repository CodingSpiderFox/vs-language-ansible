{
    "name": "ansible",
    "displayName": "language-Ansible",
    "description": "Ansible syntax highlighting support",
    "version": "0.0.1",
    "publisher": "haaaad",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "ansible",
            "aliases": ["Ansible", "ansible"],
